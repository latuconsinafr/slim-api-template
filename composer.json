{
    "require": {
        "cycle/annotated": "^2.4",
        "cycle/orm": "^1.7",
        "cycle/schema-builder": "^1.2",
        "php-di/php-di": "^6.3",
        "ramsey/uuid": "^4.2",
        "slim/psr7": "^1.5",
        "slim/slim": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
